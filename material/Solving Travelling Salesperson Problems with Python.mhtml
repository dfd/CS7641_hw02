"Paragraph:b7cea8640b2e_61":{"id":"b7cea8640b2e_61","name":"488a","type":"IFRAME","href":null,"layout":"INSET_CENTER","metadata":null,"text":"","hasDropCap":null,"dropCapImage":null,"markups":[],"__typename":"Paragraph","iframe":{"type":"id","generated":true,"id":"$Paragraph:b7cea8640b2e_61.iframe","typename":"Iframe"},"mixtapeMetadata":null},"MediaResource:cc7ee1a83e097d6d7edd758f6e252c42":{"id":"cc7ee1a83e097d6d7edd758f6e252c42","iframeSrc":"","iframeHeight":0,"iframeWidth":0,"title":"TSP Optimization Using a Genetic Algorithm - Attempt 2","__typename":"MediaResource"},"$Paragraph:b7cea8640b2e_61.iframe":{"mediaResource":{"type":"id","generated":false,"id":"MediaResource:cc7ee1a83e097d6d7edd758f6e252c42","typename":"MediaResource"},"__typename":"Iframe"},"Paragraph:b7cea8640b2e_62":{"id":"b7cea8640b2e_62","name":"6b70","type":"PRE","href":null,"layout":null,"metadata":null,"text":"The best state found is:  [7 6 5 4 3 2 1 0]","hasDropCap":null,"dropCapImage":null,"markups":[],"__typename":"Paragraph","iframe":null,"mixtapeMetadata":null},"Paragraph:b7cea8640b2e_63":{"id":"b7cea8640b2e_63","name":"1d13","type":"PRE","href":null,"layout":null,"metadata":null,"text":"The fitness at the best state is:  17.3426175477","hasDropCap":null,"dropCapImage":null,"markups":[],"__typename":"Paragraph","iframe":null,"mixtapeMetadata":null},"Paragraph:b7cea8640b2e_64":{"id":"b7cea8640b2e_64","name":"303e","type":"P","href":null,"layout":null,"metadata":null,"text":"This solution is illustrated below and can be shown to be an optimal solution to this problem.","hasDropCap":null,"dropCapImage":null,"markups":[],"__typename":"Paragraph","iframe":null,"mixtapeMetadata":null},"Paragraph:b7cea8640b2e_65":{"id":"b7cea8640b2e_65","name":"4ce3","type":"IMG","href":null,"layout":"INSET_CENTER","metadata":{"type":"id","generated":false,"id":"ImageMetadata:1*1yV8E5YsvgaIeIQWpM96Iw.jpeg","typename":"ImageMetadata"},"text":"","hasDropCap":null,"dropCapImage":null,"markups":[],"__typename":"Para